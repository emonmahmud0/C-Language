#include<stdio.h>
int main()
{
    int n,i,count;
    scanf("%d",&n);
    char arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%c",&arr[i]);
    }
    count=0;
    for(i=0;i<n;i++)
    {
        if(arr[i]=='a' || arr[i]=='e' || arr[i]=='i' || arr[i]=='o' || arr[i]=='u' || arr[i]=='A' || arr[i]=='E' ||  arr[i]=='I' || arr[i]=='O' || arr[i]=='U')
        {
           count++;

        }

    }
        printf("Count: ");
        printf("%d",count);


       return 0;
}
