#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int i,a[n],b[n],sum=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<n;i++)
    {
        sum= a[i] + b[i];
        printf("%d ",sum);
    }

        return 0;
}
